<template name="36c16146">
  <view class="_view data-v-3c1b8ecf">
    <page-head title="{{title}}" class="_page-head data-v-3c1b8ecf"></page-head>
    <view class="_view data-v-3c1b8ecf uni-padding-wrap">
      <view class="_view data-v-3c1b8ecf page-section swiper">
        <view class="_view data-v-3c1b8ecf page-section-spacing">
          <swiper class="_swiper data-v-3c1b8ecf swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
            <swiper-item class="_swiper-item data-v-3c1b8ecf">
              <view class="_view data-v-3c1b8ecf swiper-item uni-bg-red">A</view>
            </swiper-item>
            <swiper-item class="_swiper-item data-v-3c1b8ecf">
              <view class="_view data-v-3c1b8ecf swiper-item uni-bg-green">B</view>
            </swiper-item>
            <swiper-item class="_swiper-item data-v-3c1b8ecf">
              <view class="_view data-v-3c1b8ecf swiper-item uni-bg-blue">C</view>
            </swiper-item>
          </swiper>
        </view>
      </view>
    </view>
    <view class="_view data-v-3c1b8ecf swiper-list">
      <view class="_view data-v-3c1b8ecf uni-list-cell uni-list-cell-pd">
        <view class="_view data-v-3c1b8ecf uni-list-cell-db">指示点</view>
        <switch checked="{{indicatorDots}}" bindchange="handleProxy" data-eventid="{{'mKH-0'}}" data-comkey="{{$k}}" class="_switch data-v-3c1b8ecf" />
      </view>
      <view class="_view data-v-3c1b8ecf uni-list-cell uni-list-cell-pd">
        <view class="_view data-v-3c1b8ecf uni-list-cell-db">自动播放</view>
        <switch checked="{{autoplay}}" bindchange="handleProxy" data-eventid="{{'I5u-1'}}" data-comkey="{{$k}}" class="_switch data-v-3c1b8ecf" />
      </view>
    </view>
    <view class="_view data-v-3c1b8ecf uni-padding-wrap">
      <view class="_view data-v-3c1b8ecf uni-common-mt">
        <text class="_text data-v-3c1b8ecf">幻灯片切换时长(ms)</text>
        <text class="_text data-v-3c1b8ecf info">{{duration}}</text>
      </view>
      <slider bindchange="handleProxy" value="{{duration}}" min="500" max="2000" data-eventid="{{'T9U-2'}}" data-comkey="{{$k}}" class="_slider data-v-3c1b8ecf" />
      <view class="_view data-v-3c1b8ecf uni-common-mt">
        <text class="_text data-v-3c1b8ecf">自动播放间隔时长(ms)</text>
        <text class="_text data-v-3c1b8ecf info">{{interval}}</text>
      </view>
      <slider bindchange="handleProxy" value="{{interval}}" min="2000" max="10000" data-eventid="{{'iT9-3'}}" data-comkey="{{$k}}" class="_slider data-v-3c1b8ecf" />
    </view>
  </view>
</template>